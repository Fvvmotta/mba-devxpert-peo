  "components": {
    "schemas": {
      "AdicionarAulaInputDTO": {
        "required": [
          "cursoId",
          "materialUrl",
          "titulo"
        ],
        "type": "object",
        "properties": {
          "cursoId": {
            "type": "string",
            "format": "uuid"
          },
          "titulo": {
            "minLength": 1,
            "type": "string"
          },
          "descricao": {
            "type": "string",
            "nullable": true
          },
          "materialUrl": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "AtualizarAlunoIdentityDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "nome": {
            "type": "string",
            "nullable": true
          },
          "email": {
            "type": "string",
            "nullable": true
          },
          "novaSenha": {
            "type": "string",
            "nullable": true
          },
          "confirmarSenha": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "CriarMatriculaDto": {
        "required": [
          "cursoId"
        ],
        "type": "object",
        "properties": {
          "cursoId": {
            "type": "string",
            "format": "uuid"
          }
        },
        "additionalProperties": false
      },
      "CursoInputDTO": {
        "required": [
          "autor",
          "nome"
        ],
        "type": "object",
        "properties": {
          "nome": {
            "minLength": 1,
            "type": "string"
          },
          "autor": {
            "minLength": 1,
            "type": "string"
          },
          "cargaHoraria": {
            "maximum": 500,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "descricaoConteudoProgramatico": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "PagamentoPedido": {
        "type": "object",
