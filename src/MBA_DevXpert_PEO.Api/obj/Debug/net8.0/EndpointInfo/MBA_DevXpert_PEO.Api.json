          "matriculaId": {
            "type": "string",
            "format": "uuid"
          },
          "alunoId": {
            "type": "string",
            "format": "uuid"
          },
          "valor": {
            "type": "number",
            "format": "double"
          },
          "nomeCartao": {
            "type": "string",
            "nullable": true
          },
          "numeroCartao": {
            "type": "string",
            "nullable": true
          },
          "expiracaoCartao": {
            "type": "string",
            "nullable": true
          },
          "cvvCartao": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "UpdateCursoInputDTO": {
        "required": [
          "autor",
          "id",
          "nome"
        ],
        "type": "object",
        "properties": {
          "nome": {
            "minLength": 1,
            "type": "string"
          },
          "autor": {
            "minLength": 1,
            "type": "string"
          },
          "cargaHoraria": {
            "maximum": 500,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "descricaoConteudoProgramatico": {
            "type": "string",
            "nullable": true
          },
          "id": {
            "type": "string",
            "format": "uuid"
